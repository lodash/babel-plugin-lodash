{
  "name": "babel-plugin-lodash",
  "version": "3.1.0",
  "description": "Modular Lodash builds without the hassle.",
  "keywords": "babel-plugin, cherry-pick, lodash, modules",
  "repository": "lodash/babel-plugin-lodash",
  "license": "MIT",
  "main": "lib/index.js",
  "author": "Graeme Yeates <megawac@gmail.com> (https://github.com/megawac)",
  "contributors": [
    "Graeme Yeates <megawac@gmail.com> (https://github.com/megawac)",
    "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "test": "mocha --reporter min --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "~0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "lodash-compat": "^3.10.2",
    "lodash-es": "^4.11.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "glob": "^7.0.3",
    "lodash": "^4.11.2"
  },
  "babel": {
    "plugins": [
      "babel-plugin-add-module-exports"
    ],
    "presets": [
      "es2015"
    ]
  },
  "files": [
    "lib/config.js",
    "lib/importModule.js",
    "lib/index.js",
    "lib/MapCache.js",
    "lib/mapping.js",
    "lib/PackageStore.js",
    "lib/Store.js"
  ]
}
